{
  "name": "GitDocParser",
  "description": "Parse github doc and generate a sidebar",
  "version": "0.1",
  "minimum_chrome_version": "16.0.884",
  "permissions": ["<all_urls>", "tabs", "activeTab"],
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["bar.css"]
    }
  ],

  "icons": {"16": "icon16.png"}
}
