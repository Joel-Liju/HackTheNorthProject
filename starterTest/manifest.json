{
  "name": "GitDocParser",
  "description": "Parse github doc and generate a sidebar",
  "version": "0.1",
  "minimum_chrome_version": "16.0.884",
  "permissions": ["<all_urls>", "tabs"],
  "browser_action": {"default_popup": "popup.html"},
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "css":["styles.css"],
      "js":["popup.js","send_links.js"]
    }
  ]
}